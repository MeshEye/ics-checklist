extends layout

block stylesheets
	link(rel='stylesheet', href='/css/bootstrap.min.css')
	link(rel='stylesheet', href='/css/bootstrap-theme.min.css')
	link(rel='stylesheet', href='/css/ics-checklist.css')

block scripts
	script(src="/js/jquery.min.js")
	script(src="/js/bootstrap.min.js")
	script(src="/js/management.js")

block content
	h1= title
	div.row
		section#sctn_device_overview.col-md-2
			p Vendors
				p Device types:
				select#lst_device_types
					option(value=null) Loading device types...
				p Vendors:
				select#lst_vendors
					option(value=null) Select a device type first
				p Device series:
				select#lst_device_series
					option(value=null) Select a vendor first
				p Devices:
				select#lst_devices
					option(value=null) Select a device series first